.preview-tips {
    margin: 20rpx 0;
}

.video {
    margin: 50px auto;
    width: 100%;
    height: 300px;
}

.photo {
    margin-top: 80rpx;
    width: 70%;
    padding: 0 15%;
    display: flex;
    justify-content: space-between;
}

.btn {
    /* margin-top: 60rpx; */
    width: 150rpx;
    height: 150rpx;
    border-radius: 50%;
    background: #d6d1d1;
    text-align: center;
    line-height: 150rpx;
    color: #fff;
}

.back {
    margin-top: 90rpx;
    width: 70%;
    padding: 0 15%;
    display: flex;
    justify-content: space-between;
}

.cameraBox {
    position: relative;
}

.word {
    position: absolute;
    /* margin-top: -212rpx; */
    color: #fff;
    top: 415px;
    z-index: 10;
    left: 20px;

}



.canvas {
    border: 2rpx solid pink;
    background: pink;
    width: 100%;
    height: 300px;
    /* //position:fixed;left:100%; 想隐藏画布单又想执行画布方法，用这个css；用display会不执行画布 */
}


.addressBox{
    border-radius: 10rpx;
    width: 300px;
    height: 400px;
    overflow-y: scroll;
    padding: 30rpx;
    line-height: 30px;
}